<link rel="stylesheet" href="{THEME}/style/milliy.css">


{include file="modul-shyi/home-header.tpl"}


 

        <div class="header">
            <div class="container-fluid">
                <div class="header__text">
                    <h1> {title} </h1>
                </div>
                <div class="header__text-2">
                    <span>Project direction</span>
                    <span class="fw-bold">[xfvalue_project-direction-uz]</span>
                </div>
                <div class="header__img mt-2 container-fluid ">
                    <div class="row justify-content-center  position-relative overflow-hidden anim_z">
                        <img src="{image-1}" alt="" class="img-fluid reveal">
                        <div class="yellow"></div>
                    </div>
                </div>
                <div class="under__block-hr">
                    <span> {title} </span>
                </div>
            </div>
       </div>

        <div class="info__tv mb-5">
            <div class="container">
                <div class="row justify-content-between">
                    <div class="col-md-5 header__parag">
                        <p>[xfvalue_project-subtitle-en]</p>

                        <ul>
                            <li>[xfvalue_project-direction-en]</li>
                        </ul>
                        
                    </div>
                    <div class="col-md-5 header__parag-2">
                        <h4>IN BREAK</h4>
<p>
{full-story}
</p>
                    </div>
                </div>
            </div>
        </div>

     
        <div class="second__img container-fluid">
            <div class="row justify-content-center position-relative overflow-hidden anim_z ">
                <div class="yellow "></div>
                <img src="{image-2}" alt="" class="img-fluid  on reveal">
            </div>
        </div>
        <div class="third__img container-fluid mt-5 position-relative overflow-hidden anim_z ">
            <div class="row justify-content-center">
                <div class="yellow "></div>

                <img src="{image-3}" alt="" class="img-fluid  on reveal">
            </div>
        </div>
        <div class="fourth__img container-fluid mt-5 position-relative overflow-hidden anim_z ">
            <div class="row justify-content-center ">
                <div class="yellow "></div>

                <img src="{image-4}" alt="" class="img-fluid on  reveal">
            </div>
        </div>
        <div class="fifth__img container-fluid mt-5  ">
            <div class="row justify-content-center position-relative overflow-hidden anim_z">
                   [xfgiven_youtube]    <iframe width="100%" height="600" src="https://www.youtube.com/embed/[xfvalue_youtube]" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope;
                   picture-in-picture" allowfullscreen></iframe>[/xfgiven_youtube]
  
                <div class="yellow "></div>
            </div>
        </div>
      
      
      
      
      
      
      

